{
  "configurations": [
    {
      "name": "Python Debugger: Flask",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "app\\__init__.py",
        "FLASK_DEBUG": "1"
      },
      "args": ["run"],
      "jinja": true,
      "autoStartBrowser": false,
      "justMyCode": false,
      "showReturnValue": true
    }
  ]
}
